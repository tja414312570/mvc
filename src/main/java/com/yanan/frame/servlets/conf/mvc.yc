###################### plugin
plugins:[
		## interface
		"com.yanan.frame.servlets.response.MethodAnnotationType",
		"com.yanan.frame.servlets.parameter.ParameterAnnotationType",
		com.yanan.frame.servlets.response.ResponseTypeSupportAnnotation,
		## dispatcher
		{ ##Restful调配器 @RequestMapping @GetMapping @PostMapping @DeleteMapping @RequestParam @RequestPath
			class:com.yanan.frame.servlets.RestfulDispatcher,
			priority:0,
			signlton:true,
			attribute:RESTFUL_STYLE,
			service:"javax.servlet.Servlet,com.yanan.frame.servlets.ServletDispatcher"
		},
		## 响应工具
		com.yanan.frame.servlets.response.FileResponseHandler,
		com.yanan.frame.servlets.response.ForwardResponseHandler,
		com.yanan.frame.servlets.response.JsonResponseHandler,
		com.yanan.frame.servlets.response.RedirectResponseHandler,
		com.yanan.frame.servlets.response.ResponseStatusHandler,
		## 异常响应工具
		com.yanan.frame.servlets.exception.ServletExceptionHandler,
		com.yanan.frame.servlets.exception.DefaultExceptionHandler,
		## 参数验证工具
		com.yanan.frame.servlets.validator.DefaultParameterValitationHandler,
		## 默认参数注入工具
		com.yanan.frame.servlets.parameter.DefaultParameterHandler,
		## 默认参数注解类型
		com.yanan.frame.servlets.parameter.RestfulParameter,
		com.yanan.frame.servlets.session.parameter.TokenParameterHandler
	]
mvc:{
	server:{
			tomcat:[
				{ ## 默认调配器
					class:org.apache.catalina.servlets.DefaultServlet,
					priority:2,
					signlton:true,
					attribute:"*",
					service:javax.servlet.Servlet
				},
				{ ## 默认JSP调配器
					class:org.apache.jasper.servlet.JspServlet,
					priority:1,
					signlton:true,
					attribute:".jsp",
					service:javax.servlet.Servlet
				},
			]
		},
	response:{
		fileResponse:{
			pools:10,
			buffer:4096,
		}
	}
	}
	